{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfroihaan"
		},
		"ADLS_roihaan_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_roihaan'"
		},
		"SQL_roihaan_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQL_roihaan'"
		},
		"ADLS_roihaan_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccroihaan.dfs.core.windows.net/"
		},
		"SQL_roihaan_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "roihaan.database.windows.net"
		},
		"SQL_roihaan_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "data"
		},
		"SQL_roihaan_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "roihaan"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfroihaan_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_roihaan_accountKey": {
						"value": "[parameters('ADLS_roihaan_accountKey')]"
					},
					"SQL_roihaan_password": {
						"value": "[parameters('SQL_roihaan_password')]"
					},
					"ADLS_roihaan_properties_typeProperties_url": {
						"value": "[parameters('ADLS_roihaan_properties_typeProperties_url')]"
					},
					"SQL_roihaan_properties_typeProperties_server": {
						"value": "[parameters('SQL_roihaan_properties_typeProperties_server')]"
					},
					"SQL_roihaan_properties_typeProperties_database": {
						"value": "[parameters('SQL_roihaan_properties_typeProperties_database')]"
					},
					"SQL_roihaan_properties_typeProperties_userName": {
						"value": "[parameters('SQL_roihaan_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}