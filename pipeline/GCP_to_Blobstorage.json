{
	"name": "GCP_to_Blobstorage",
	"properties": {
		"activities": [
			{
				"name": "GCS_BlobStorag",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "GoogleCloudStorageReadSettings",
							"recursive": true
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "GCS_BlobStorage",
						"type": "DatasetReference",
						"parameters": {
							"folder_name": {
								"value": "@pipeline().parameters.folder_name",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"folder_name": {
				"type": "string"
			},
			"file_name": {
				"type": "string"
			}
		},
		"annotations": []
	}
}